{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/listing", "dest": "/listing.html" },
    { "src": "/car", "dest": "/car.html" },
    { "src": "/(.*)", "dest": "/$1.html" }
  ]
}
